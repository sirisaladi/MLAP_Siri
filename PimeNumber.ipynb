   "source": [
    "x=int(input(\"enter a number\"))\n",
    "count=0\n",
    "n=1\n",
    "while n<=x:\n",
    "    if x%n==0:\n",
    "        count=count+1\n",
    "        n=n+1\n",
    "    else:\n",
    "        n=n+1\n",
    "if count==2:\n",
    "    print(\"prime number\")\n",
    "else:\n",
    "    print(\"not a prime number\")"
